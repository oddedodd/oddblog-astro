---
import { sanityClient } from "sanity:client";

const posts = await sanityClient.fetch(`
  *[_type == "post"] {
    title,
    slug,
    body
  }
`);

console.log(posts);

---

<h1>Blog test</h1>
<ul>
{posts.map((post) => (
  <li>
    <h2>{post.title}</h2>
    <p>{post.body}</p>
  </li>
))}
</ul>